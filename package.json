{
  "name": "convert",
  "version": "1.0.0",
  "description": "Convert - Universal File Conversion Tool. A powerful desktop application for converting between various file formats including documents, images, videos, and more. Built with Electron and React for cross-platform compatibility.",
  "main": "public/electron.js",
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "https://github.com/convert-app/convert.git"
  },
  "bugs": {
    "url": "https://github.com/convert-app/convert/issues"
  },
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "build:clean": "npm run clean && npm run build",
    "electron": "NODE_ENV=production electron .",
    "electron-dev": "cross-env ELECTRON_IS_DEV=true NODE_ENV=development electron .",
    "dev": "concurrently \"npm run start\" \"wait-on http://localhost:3000 && npm run electron-dev\"",
    "dev:renderer": "webpack serve --mode development",
    "dev:main": "cross-env ELECTRON_IS_DEV=true NODE_ENV=development nodemon --watch public/electron.js --watch src/shared --exec electron .",
    "dev:full": "concurrently \"npm run dev:renderer\" \"wait-on http://localhost:3000 && npm run dev:main\"",
    "dist": "npm run build:clean && electron-builder",
    "dist:mac": "npm run build:clean && electron-builder --mac",
    "dist:mac-dmg": "npm run build:clean && electron-builder --mac dmg",
    "dist:mac-zip": "npm run build:clean && electron-builder --mac zip",
    "dist:mac-universal": "npm run build:clean && electron-builder --mac --universal",
    "pack": "npm run build:clean && electron-builder --dir",
    "pack:mac": "npm run build:clean && electron-builder --mac --dir",
    "clean": "rm -rf build dist .electron-cache",
    "clean:build": "rm -rf build",
    "clean:dist": "rm -rf dist",
    "troubleshoot": "node scripts/troubleshoot-build.js",
    "lint": "echo \"Linting not configured yet\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:dev": "node src/shared/tests/devEnvironment.test.js",
    "test:unit": "jest --testPathPattern=\"(test|spec)\\.(js|jsx)$\"",
    "test:e2e": "node src/tests/e2e/test-runner.js",
    "test:e2e:conversion": "node src/tests/e2e/test-runner.js --suite conversion-e2e.test.js",
    "test:e2e:file-handling": "node src/tests/e2e/test-runner.js --suite file-handling-e2e.test.js",
    "test:e2e:quick": "node src/tests/e2e/test-runner.js --timeout 15000 --retries 1 --no-coverage",
    "test:e2e:full": "node src/tests/e2e/test-runner.js --timeout 60000 --retries 3 --parallel"
  },
  "keywords": [
    "electron",
    "react",
    "file-conversion",
    "desktop-app",
    "document-converter",
    "image-converter",
    "video-converter",
    "pdf-converter",
    "productivity",
    "utility",
    "cross-platform",
    "macos",
    "ffmpeg"
  ],
  "author": {
    "name": "Convert App Team",
    "email": "support@convertapp.com",
    "url": "https://convertapp.com"
  },
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@sentry/electron": "^6.11.0",
    "canvas": "^3.2.0",
    "electron-updater": "^6.6.2",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "mammoth": "^1.10.0",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^5.4.149",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "sharp": "^0.34.3",
    "task-master-ai": "^0.25.1",
    "turndown": "^7.2.1",
    "yauzl": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "babel-loader": "^10.0.0",
    "buffer": "^6.0.3",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "electron-debug": "^4.1.0",
    "electron-reload": "^2.0.0-alpha.1",
    "events": "^3.3.0",
    "global": "^4.4.0",
    "html-webpack-plugin": "^5.6.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.1.1",
    "nodemon": "^3.1.10",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "style-loader": "^4.0.0",
    "util": "^0.12.5",
    "wait-on": "^8.0.4",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "build": {
    "appId": "com.convertapp.convert",
    "productName": "Convert",
    "copyright": "Copyright Â© 2024 Convert App Team. All rights reserved.",
    "directories": {
      "output": "dist",
      "buildResources": "assets"
    },
    "asar": true,
    "asarUnpack": [
      "node_modules/ffmpeg-static/**/*",
      "node_modules/canvas/**/*"
    ],
    "buildDependenciesFromSource": false,
    "nodeGypRebuild": false,
    "files": [
      "build/**/*",
      "public/electron.js",
      "public/preload.js",
      "src/**/*",
      "node_modules/**/*",
      "!node_modules/.cache",
      "!node_modules/@img/**/*",
      "!node_modules/@biomejs/cli-linux-x64",
      "!node_modules/@anthropic-ai/**/*",
      "!node_modules/@ai-sdk/**/*",
      "!node_modules/task-master-ai/**/*",
      "!node_modules/@testing-library/**/*",
      "!node_modules/jest/**/*",
      "!node_modules/babel-loader/**/*",
      "!node_modules/@babel/**/*",
      "!node_modules/webpack/**/*",
      "!node_modules/webpack-cli/**/*",
      "!node_modules/webpack-dev-server/**/*",
      "!node_modules/electron-builder/**/*",
      "!node_modules/concurrently/**/*",
      "!node_modules/nodemon/**/*",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "assets/icon.icns",
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "identity": null,
      "target": [
        {
          "target": "dmg",
          "arch": "arm64"
        },
        {
          "target": "zip",
          "arch": "arm64"
        }
      ]
    },
    "dmg": {
      "title": "${productName} ${version} - Universal File Converter",
      "icon": "assets/icon.icns",
      "background": null,
      "contents": [
        {
          "x": 130,
          "y": 220,
          "type": "file"
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "window": {
        "width": 540,
        "height": 380
      },
      "format": "UDZO"
    },
    "publish": {
      "provider": "github",
      "owner": "convert-app",
      "repo": "convert",
      "private": false,
      "releaseType": "release",
      "publishAutoUpdate": true
    },
    "compression": "normal",
    "artifactName": "${productName}-${version}-${arch}.${ext}",
    "buildVersion": "1.0.0",
    "npmRebuild": false,
    "electronDownload": {
      "cache": "./.electron-cache"
    }
  }
}
