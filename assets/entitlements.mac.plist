<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <!-- Allow JIT compilation for V8 engine -->
  <key>com.apple.security.cs.allow-jit</key>
  <true/>
  
  <!-- Allow unsigned executable memory for Node.js native modules -->
  <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
  <true/>
  
  <!-- Disable library validation for compatibility -->
  <key>com.apple.security.cs.disable-library-validation</key>
  <true/>
  
  <!-- Allow DYLD environment variables -->
  <key>com.apple.security.cs.allow-dyld-environment-variables</key>
  <true/>
  
  <!-- Network access for file conversion services -->
  <key>com.apple.security.network.client</key>
  <true/>
  
  <!-- File system access for file conversion -->
  <key>com.apple.security.files.user-selected.read-write</key>
  <true/>
  
  <!-- Downloads folder access -->
  <key>com.apple.security.files.downloads.read-write</key>
  <true/>
  
  <!-- Camera access (if needed for document scanning) -->
  <key>com.apple.security.device.camera</key>
  <true/>
  
  <!-- Microphone access (if needed for audio conversion) -->
  <key>com.apple.security.device.microphone</key>
  <true/>
</dict>
</plist>